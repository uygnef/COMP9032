                                 ;----copy form http://webapps.cse.unsw.edu.au/webcms2/course/index.php?cid=2446&color=teal ;
                                 ;writer by professor in COMP9032
                                 ;modify by Feng Yu
                                 ; The program gets input from keypad and displays its ascii value on LEDs
                                 ; Port F is used for keypad, high 4 bits for column selection, low four bits for reading rows
                                 ; Port C is used to display the ASCII value of a key.
                                 ; Date: Aug 10, 2015
                                 
                                 get_value_from_keypad:	
00024c ef40                      		ldi temp1, PORTFDIR			; columns are outputs, rows are inputs
00024d 9340 010a                 		sts	DDRL, temp1
                                 	
                                 	keypad_main:
00024f ee3f                      		ldi cmask, INITCOLMASK		; initial column mask
000250 2711                      		clr	col						; initial column
                                 	colloop:
000251 3014                      		cpi col, 4
000252 f1b1                      		breq none_press
000253 9330 010b                 		sts	PORTL, cmask				; set column to mask value (one column off)
000255 ef4f                      		ldi temp1, 0xFF
                                 	key_pad_delay:
000256 954a                      		dec temp1
000257 f7f1                      		brne key_pad_delay
000258 9140 0109                 		lds	temp1, PINL				; read PORTL
00025a 704f                      		andi temp1, ROWMASK
00025b 304f                      		cpi temp1, 0xF				; check if any rows are on
00025c f059                      		breq nextcol
                                 									; if yes, find which row is on
00025d e021                      		ldi rmask, INITROWMASK		; initialise row check
00025e 2700                      		clr	row						; initial row
                                 
                                 	rowloop:
00025f 3004                      		cpi row, 4
000260 f039                      		breq nextcol
000261 2f54                      		mov temp2, temp1
000262 2352                      		and temp2, rmask				; check masked bit
000263 f049                      		breq convert 				; if bit is clear, convert the bitcode
000264 9503                      		inc row						; else move to the next row
000265 0f22                      		lsl rmask					; shift the mask to the next bit
000266 940c 025f                 		jmp rowloop
                                 
                                 	nextcol:
000268 0f33                      		lsl cmask					; else get new mask by shifting and 
000269 9533                      		inc cmask
00026a 9513                      		inc col						; increment column value
00026b 940c 0251                 		jmp colloop					; and check the next column
                                 
                                 	convert:
00026d 3013                      		cpi col, 3					; if column is 3 we have a letter
00026e f049                      		breq letters				
00026f 3003                      		cpi row, 3					; if row is 3 we have a symbol or 0
000270 f059                      		breq symbols
                                 
000271 2f40                      		mov temp1, row				; otherwise we have a number in 1-9
000272 0f44                      		lsl temp1
000273 0f40                      		add temp1, row				; temp1 = row * 3
000274 0f41                      		add temp1, col				; add the column address to get the value
000275 5c4f                      		subi temp1, -'1'			; add the value of character '0'
000276 940c 028b                 		jmp convert_end
                                 
                                 	letters:
000278 e441                      		ldi temp1, 'A'
000279 0f40                      		add temp1, row				; increment the character 'A' by the row value
00027a 940c 028b                 		jmp convert_end
                                 
                                 	symbols:
00027c 3010                      		cpi col, 0					; check if we have a star
00027d f029                      		breq star
00027e 3011                      		cpi col, 1					; or if we have zero
00027f f031                      		breq zero					
000280 e243                      		ldi temp1, '#'				; if not we have hash
000281 940c 028b                 		jmp convert_end
                                 	star:
000283 e24a                      		ldi temp1, '*'				; set to star
000284 940c 028b                 		jmp convert_end
                                 	zero:
000286 e340                      		ldi temp1, '0'				; set to zero
000287 940c 028b                 		jmp convert_end
                                 	
                                 	none_press:
000289 e244                      		ldi temp1, '$'
00028a 9508                      		ret
                                 	
                                 	convert_end:
00028b 9508                      		ret		
                                 
