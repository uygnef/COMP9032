                                 ;store all macros
                                 .macro st2				;-----store 2 bytes in data memory-------;
                                 	ldi r31, high(@2)
                                 	ldi r30, low(@2)
                                 	st z+, @0			; st2 register1 ==> data memory1
                                 	st z, @1			;	  register2 ==> data memory2
                                 .endmacro
                                 
                                 .macro ld2				;-----load 2 bytes from data memory to 2 register
                                 	ldi r31, high(@0)
                                 	ldi r30, low(@0)	;	data memory => register1, register2
                                 	ld @1, z+
                                 	ld @2, z
                                 .endmacro
                                 
                                 .macro STORE
                                 .if @0 > 63
                                 sts @0, @1
                                 .else
                                 out @0, @1
                                 .endif
                                 .endmacro
                                 
                                 .macro LOAD
                                 .if @1 > 63
                                 lds @0, @1
                                 .else
                                 in @0, @1
                                 .endif
                                 .endmacro
                                 
