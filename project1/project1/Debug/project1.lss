                                 ;----copy form http://webapps.cse.unsw.edu.au/webcms2/course/index.php?cid=2446&color=teal ;
                                 ;writer by professor in COMP9032
                                 ;modify by Feng Yu
                                 ; The program gets input from keypad and displays its ascii value on LEDs
                                 ; Port F is used for keypad, high 4 bits for column selection, low four bits for reading rows
                                 ; Port C is used to display the ASCII value of a key.
                                 ; Date: Aug 10, 2015
                                 
                                 get_value_from_keypad:	
0001e5 ef40                      		ldi temp1, PORTFDIR			; columns are outputs, rows are inputs
0001e6 9340 010a                 		sts	DDRL, temp1
                                 	
                                 	keypad_main:
0001e8 ee3f                      		ldi cmask, INITCOLMASK		; initial column mask
0001e9 2711                      		clr	col						; initial column
                                 	colloop:
0001ea 3014                      		cpi col, 4
0001eb f1b1                      		breq none_press
0001ec 9330 010b                 		sts	PORTL, cmask				; set column to mask value (one column off)
0001ee ef4f                      		ldi temp1, 0xFF
                                 	key_pad_delay:
0001ef 954a                      		dec temp1
0001f0 f7f1                      		brne key_pad_delay
0001f1 9140 0109                 		lds	temp1, PINL				; read PORTL
0001f3 704f                      		andi temp1, ROWMASK
0001f4 304f                      		cpi temp1, 0xF				; check if any rows are on
0001f5 f059                      		breq nextcol
                                 									; if yes, find which row is on
0001f6 e021                      		ldi rmask, INITROWMASK		; initialise row check
0001f7 2700                      		clr	row						; initial row
                                 
                                 	rowloop:
0001f8 3004                      		cpi row, 4
0001f9 f039                      		breq nextcol
0001fa 2f54                      		mov temp2, temp1
0001fb 2352                      		and temp2, rmask				; check masked bit
0001fc f049                      		breq convert 				; if bit is clear, convert the bitcode
0001fd 9503                      		inc row						; else move to the next row
0001fe 0f22                      		lsl rmask					; shift the mask to the next bit
0001ff 940c 01f8                 		jmp rowloop
                                 
                                 	nextcol:
000201 0f33                      		lsl cmask					; else get new mask by shifting and 
000202 9533                      		inc cmask
000203 9513                      		inc col						; increment column value
000204 940c 01ea                 		jmp colloop					; and check the next column
                                 
                                 	convert:
000206 3013                      		cpi col, 3					; if column is 3 we have a letter
000207 f049                      		breq letters				
000208 3003                      		cpi row, 3					; if row is 3 we have a symbol or 0
000209 f059                      		breq symbols
                                 
00020a 2f40                      		mov temp1, row				; otherwise we have a number in 1-9
00020b 0f44                      		lsl temp1
00020c 0f40                      		add temp1, row				; temp1 = row * 3
00020d 0f41                      		add temp1, col				; add the column address to get the value
00020e 5c4f                      		subi temp1, -'1'			; add the value of character '0'
00020f 940c 0224                 		jmp convert_end
                                 
                                 	letters:
000211 e441                      		ldi temp1, 'A'
000212 0f40                      		add temp1, row				; increment the character 'A' by the row value
000213 940c 0224                 		jmp convert_end
                                 
                                 	symbols:
000215 3010                      		cpi col, 0					; check if we have a star
000216 f029                      		breq star
000217 3011                      		cpi col, 1					; or if we have zero
000218 f031                      		breq zero					
000219 e243                      		ldi temp1, '#'				; if not we have hash
00021a 940c 0224                 		jmp convert_end
                                 	star:
00021c e24a                      		ldi temp1, '*'				; set to star
00021d 940c 0224                 		jmp convert_end
                                 	zero:
00021f e340                      		ldi temp1, '0'				; set to zero
000220 940c 0224                 		jmp convert_end
                                 	
                                 	none_press:
000222 e244                      		ldi temp1, '$'
000223 9508                      		ret
                                 	
                                 	convert_end:
000224 9508                      		ret		
                                 
