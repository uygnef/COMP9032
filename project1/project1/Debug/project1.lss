                                 ;----copy form http://webapps.cse.unsw.edu.au/webcms2/course/index.php?cid=2446&color=teal ;
                                 ;writer by professor in COMP9032
                                 ;modify by Feng Yu
                                 ; The program gets input from keypad and displays its ascii value on LEDs
                                 ; Port F is used for keypad, high 4 bits for column selection, low four bits for reading rows
                                 ; Port C is used to display the ASCII value of a key.
                                 ; Date: Aug 10, 2015
                                 
                                 get_key:	
000351 ef40                      		ldi temp1, PORTFDIR			; columns are outputs, rows are inputs
000352 9340 010a                 		sts	DDRL, temp1
                                 	
                                 	keypad_main:
000354 ee3f                      		ldi cmask, INITCOLMASK		; initial column mask
000355 2711                      		clr	col						; initial column
                                 	colloop:
000356 3014                      		cpi col, 4
000357 f1b1                      		breq none_press
000358 9330 010b                 		sts	PORTL, cmask				; set column to mask value (one column off)
00035a ef4f                      		ldi temp1, 0xFF
                                 	key_pad_delay:
00035b 954a                      		dec temp1
00035c f7f1                      		brne key_pad_delay
00035d 9140 0109                 		lds	temp1, PINL				; read PORTL
00035f 704f                      		andi temp1, ROWMASK
000360 304f                      		cpi temp1, 0xF				; check if any rows are on
000361 f059                      		breq nextcol
                                 									; if yes, find which row is on
000362 e021                      		ldi rmask, INITROWMASK		; initialise row check
000363 2700                      		clr	row						; initial row
                                 
                                 	rowloop:
000364 3004                      		cpi row, 4
000365 f039                      		breq nextcol
000366 2f54                      		mov temp2, temp1
000367 2352                      		and temp2, rmask				; check masked bit
000368 f049                      		breq convert 				; if bit is clear, convert the bitcode
000369 9503                      		inc row						; else move to the next row
00036a 0f22                      		lsl rmask					; shift the mask to the next bit
00036b 940c 0364                 		jmp rowloop
                                 
                                 	nextcol:
00036d 0f33                      		lsl cmask					; else get new mask by shifting and 
00036e 9533                      		inc cmask
00036f 9513                      		inc col						; increment column value
000370 940c 0356                 		jmp colloop					; and check the next column
                                 
                                 	convert:
000372 3013                      		cpi col, 3					; if column is 3 we have a letter
000373 f049                      		breq letters				
000374 3003                      		cpi row, 3					; if row is 3 we have a symbol or 0
000375 f059                      		breq symbols
                                 
000376 2f40                      		mov temp1, row				; otherwise we have a number in 1-9
000377 0f44                      		lsl temp1
000378 0f40                      		add temp1, row				; temp1 = row * 3
000379 0f41                      		add temp1, col				; add the column address to get the value
00037a 5c4f                      		subi temp1, -'1'			; add the value of character '0'
00037b 940c 0390                 		jmp convert_end
                                 
                                 	letters:
00037d e441                      		ldi temp1, 'A'
00037e 0f40                      		add temp1, row				; increment the character 'A' by the row value
00037f 940c 0390                 		jmp convert_end
                                 
                                 	symbols:
000381 3010                      		cpi col, 0					; check if we have a star
000382 f029                      		breq star
000383 3011                      		cpi col, 1					; or if we have zero
000384 f031                      		breq zero					
000385 e243                      		ldi temp1, '#'				; if not we have hash
000386 940c 0390                 		jmp convert_end
                                 	star:
000388 e24a                      		ldi temp1, '*'				; set to star
000389 940c 0390                 		jmp convert_end
                                 	zero:
00038b e340                      		ldi temp1, '0'				; set to zero
00038c 940c 0390                 		jmp convert_end
                                 	
                                 	none_press:
00038e e244                      		ldi temp1, '$'
00038f 9508                      		ret	
                                 	convert_end:
000390 9508                      		ret
                                 
                                 have_got_key:	;make sure have press button
000391 dfbf                      	rcall get_key
000392 3244                      	cpi temp1, '$'
000393 f411                      	brne store_key
000394 940c 0391                 	jmp have_got_key
                                 	store_key:
000396 9340 021b                 		sts key_button, temp1
000398 dfb8                      		rcall get_key
000399 3244                      		cpi temp1, '$'
00039a f7d9                      		brne store_key
