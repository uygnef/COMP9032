                                 ;----copy form http://webapps.cse.unsw.edu.au/webcms2/course/index.php?cid=2446&color=teal ;
                                 ;writer by professor in COMP9032
                                 ;modify by Feng Yu
                                 ; The program gets input from keypad and displays its ascii value on LEDs
                                 ; Port F is used for keypad, high 4 bits for column selection, low four bits for reading rows
                                 ; Port C is used to display the ASCII value of a key.
                                 ; Date: Aug 10, 2015
                                 
                                 get_value_from_keypad:	
0001d6 ef40                      		ldi temp1, PORTFDIR			; columns are outputs, rows are inputs
0001d7 9340 010a                 		sts	DDRL, temp1
                                 	
                                 	keypad_main:
0001d9 ee3f                      		ldi cmask, INITCOLMASK		; initial column mask
0001da 2711                      		clr	col						; initial column
                                 	colloop:
0001db 3014                      		cpi col, 4
0001dc f1b1                      		breq none_press
0001dd 9330 010b                 		sts	PORTL, cmask				; set column to mask value (one column off)
0001df ef4f                      		ldi temp1, 0xFF
                                 	key_pad_delay:
0001e0 954a                      		dec temp1
0001e1 f7f1                      		brne key_pad_delay
0001e2 9140 0109                 		lds	temp1, PINL				; read PORTL
0001e4 704f                      		andi temp1, ROWMASK
0001e5 304f                      		cpi temp1, 0xF				; check if any rows are on
0001e6 f059                      		breq nextcol
                                 									; if yes, find which row is on
0001e7 e021                      		ldi rmask, INITROWMASK		; initialise row check
0001e8 2700                      		clr	row						; initial row
                                 
                                 	rowloop:
0001e9 3004                      		cpi row, 4
0001ea f039                      		breq nextcol
0001eb 2f54                      		mov temp2, temp1
0001ec 2352                      		and temp2, rmask				; check masked bit
0001ed f049                      		breq convert 				; if bit is clear, convert the bitcode
0001ee 9503                      		inc row						; else move to the next row
0001ef 0f22                      		lsl rmask					; shift the mask to the next bit
0001f0 940c 01e9                 		jmp rowloop
                                 
                                 	nextcol:
0001f2 0f33                      		lsl cmask					; else get new mask by shifting and 
0001f3 9533                      		inc cmask
0001f4 9513                      		inc col						; increment column value
0001f5 940c 01db                 		jmp colloop					; and check the next column
                                 
                                 	convert:
0001f7 3013                      		cpi col, 3					; if column is 3 we have a letter
0001f8 f049                      		breq letters				
0001f9 3003                      		cpi row, 3					; if row is 3 we have a symbol or 0
0001fa f059                      		breq symbols
                                 
0001fb 2f40                      		mov temp1, row				; otherwise we have a number in 1-9
0001fc 0f44                      		lsl temp1
0001fd 0f40                      		add temp1, row				; temp1 = row * 3
0001fe 0f41                      		add temp1, col				; add the column address to get the value
0001ff 5c4f                      		subi temp1, -'1'			; add the value of character '0'
000200 940c 0215                 		jmp convert_end
                                 
                                 	letters:
000202 e441                      		ldi temp1, 'A'
000203 0f40                      		add temp1, row				; increment the character 'A' by the row value
000204 940c 0215                 		jmp convert_end
                                 
                                 	symbols:
000206 3010                      		cpi col, 0					; check if we have a star
000207 f029                      		breq star
000208 3011                      		cpi col, 1					; or if we have zero
000209 f031                      		breq zero					
00020a e243                      		ldi temp1, '#'				; if not we have hash
00020b 940c 0215                 		jmp convert_end
                                 	star:
00020d e24a                      		ldi temp1, '*'				; set to star
00020e 940c 0215                 		jmp convert_end
                                 	zero:
000210 e340                      		ldi temp1, '0'				; set to zero
000211 940c 0215                 		jmp convert_end
                                 	
                                 	none_press:
000213 e244                      		ldi temp1, '$'
                                 		;sts conduct, temp1
000214 9508                      		ret
                                 	
                                 	convert_end:
                                 		;sts conduct, temp1
000215 9508                      		ret		
                                 
